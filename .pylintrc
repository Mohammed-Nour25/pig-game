[MASTER]
ignore=venv,.venv,build,dist,doc,htmlcov

[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    too-few-public-methods,
    too-many-instance-attributes,
    too-many-arguments,
    too-many-locals,
    too-many-branches

[FORMAT]
max-line-length=100
